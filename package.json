{
  "name": "web-picture-album",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "format": "prettier --write . ; eslint --fix . --ext .ts",
    "lint": "eslint . --ext .ts",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.6",
    "@angular/cdk": "^12.1.2",
    "@angular/common": "~12.2.6",
    "@angular/compiler": "~12.2.6",
    "@angular/core": "~12.2.6",
    "@angular/forms": "~12.2.6",
    "@angular/material": "^12.1.2",
    "@angular/platform-browser": "~12.2.6",
    "@angular/platform-browser-dynamic": "~12.2.6",
    "@angular/router": "~12.2.6",
    "rxjs": "~7.3.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.4.1",
    "@angular-eslint/eslint-plugin-template": "12.4.1",
    "@angular-eslint/schematics": "12.4.1",
    "@angular-eslint/template-parser": "12.4.1",
    "@angular/cli": "^12.1.2",
    "@angular/compiler-cli": "~12.2.6",
    "@types/jasmine": "~3.9.1",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.1",
    "commitizen": "^4.2.4",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-unused-imports": "^1.1.2",
    "husky": "^7.0.1",
    "jasmine-core": "~3.9.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "typescript": "~4.4.3"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
